{
    "tokens": {
        "iamatoken": {
            "doc": "Some useful documentation",
            "scopes": [
                "scope1"
            ]
        }
    },
    "volumes": {
        "disk": {
            "fs.root": "/",
            "type": "fs"
        },
        "nodes": {
            "nodes.data": {
                "testnode": {
                    "a": 12,
                    "b": {
                        "c": [
                            0,
                            1,
                            2,
                            3
                        ],
                        "d": {
                            "e": null,
                            "f": []
                        },
                        "dict": [
                            {
                                "hello": "world",
                                "test": 1
                            },
                            {
                                "complex": "yes"
                            }
                        ]
                    },
                    "path.with.dots": {
                        "nested": "a simple string"
                    },
                    "path/with/slashes": {
                        "nested": -42
                    }
                }
            },
            "nodes.handlers": {
                "/data/log": {
                    "history": 100,
                    "toString": "[{level}] [{name}] [{source}] {message}"
                }
            },
            "nodes.records": {
                "path": "./nodes.records"
            },
            "type": "nodes"
        },
        "nodes_mirror": {
            "nodes.records": {
                "path": "./nodes_mirror.records"
            },
            "nodes.sources": {
                "nodes": {
                    "host": "http://localhost:8081",
                    "type": "nodes"
                }
            },
            "type": "nodes"
        },
        "nodes_private": {
            "nodes.records": {
                "path": "./nodes_private.records"
            },
            "private": true,
            "type": "nodes"
        }
    }
}