
namespace example;


component Hello {
interface:
	method run();
	send = Float;
	
composition:
	this.run();
	
}

component World {
interface:
	receive = const Float;
	
}

composition {
	hello = Hello() [executor(second.executor)];
	hello.run();
	world = World();
	connect(hello.send, world.receive);
	
}


